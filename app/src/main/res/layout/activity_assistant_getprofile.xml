<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/ass_get_parent"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/iv_upload_profile_photo"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="center"
                    android:layout_margin="10dp"
                    android:src="@drawable/ic_profilelogo" />
                <TextView
                    android:id="@+id/tv_userName_edit"
                    android:text="USER NAME"
                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:textColor="@color/gray_color"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tv_edit_text"
                    android:text="EDIT"
                    android:textSize="20sp"
                    android:textColor="@color/Orange"
                    android:textStyle="bold"
                    android:gravity="right"
                    android:layout_gravity="right"
                    android:paddingRight="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@color/Orange"
                    />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:background="@color/gray_use"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:src="@drawable/ic_name"
                            android:layout_marginLeft="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:text="Name"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10dp"
                            android:textSize="@dimen/activity_fifteen_textSize"
                            android:textColor="@color/gray_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <TextView
                        android:id="@+id/tv_name"
                        android:text=""
                        android:textStyle="bold"
                        android:textColor="@color/gray_color"
                        android:layout_width="0dp"
                        android:layout_weight="1"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:textAlignment="center"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content">

                    </TextView>
                </LinearLayout>
                <LinearLayout
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:padding="10dp"
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </LinearLayout>
                <LinearLayout
                    android:baselineAligned="false"

                    android:orientation="horizontal"
                    android:background="@color/gray_use"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:src="@drawable/ic_mobileno"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:text="Phone no"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10dp"

                            android:textSize="@dimen/activity_fifteen_textSize"
                            android:layout_marginStart="10dp"
                            android:textColor="@color/gray_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <TextView
                        android:id="@+id/tv_phone"
                        android:text=""
                        android:textStyle="bold"
                        android:textColor="@color/gray_color"
                        android:layout_width="0dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content">

                    </TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_email"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="E-mail"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_email"
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"
                    android:layout_weight="1"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:textStyle="bold"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_experience"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="Experience"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_experience"
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:layout_weight="1"
                    android:textStyle="bold"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_city"
                        android:layout_marginLeft="10dp"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="City"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_city"
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:layout_weight="1"
                    android:textStyle="bold"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_marginLeft="10dp"

                        android:src="@drawable/ic_state"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="State"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_state"
                    android:text=""
                    android:textStyle="bold"
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"
                    android:layout_weight="1"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_billing"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="Billing"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_billing"
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textStyle="bold"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_about"
                        android:layout_marginLeft="10dp"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="About"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"

                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"
                    android:textAlignment="center"
                    android:layout_weight="1"
                    android:id="@+id/tv_about"

                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:textStyle="bold"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:padding="10dp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </LinearLayout>
            <LinearLayout
                android:baselineAligned="false"

                android:orientation="horizontal"
                android:background="@color/gray_use"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ic_typesofgigs"
                        android:layout_marginLeft="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:text="Types of Gigs"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:textSize="@dimen/activity_fifteen_textSize"
                        android:textColor="@color/gray_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_type_gigs"
                    android:text=""
                    android:textColor="@color/gray_color"
                    android:layout_width="0dp"
                    android:textStyle="bold"
                    android:textSize="@dimen/activity_fifteen_textSize"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content">

                </TextView>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>


</LinearLayout>